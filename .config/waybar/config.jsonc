{
  "position": "top",
  "modules-left": ["hyprland/workspaces"],
  "modules-center": ["hyprland/window"],
  "modules-right": ["pulseaudio", "clock"],
  "pulseaudio": {
      "format": "{icon}  {volume}%",
      "format-muted": "",
      "format-icons": {
          "default": ["", "", " "]
      },
      "on-click": "pavucontrol"
  },
  "clock": {
    "format": "<span>   </span>{:%A, %b %d  %H:%M}",
    "tooltip-format": "<big>{:%Y %B}</big>\n<tt><large>{calendar}</large></tt>"
  }
}
